module Atomic {
	const short SS = 1;
	const long LL = 1;
	const unsigned short US = 1;
	const unsigned long UL = 1;
	const float FF = 1.1;
	const double DD = 1.2;
	const char CC = 'a';
	const boolean BB = TRUE;
	
	interface test {
	  short Short(in short a,
		      inout short b,
		      out short c);
	  long Long(in long a,
		    inout long b,
		    out long c);
	  unsigned short UShort(in unsigned short a, 
			       inout unsigned short b, 
			       out unsigned short c);
	  unsigned long ULong(in unsigned long a,
			      inout unsigned long b, 
			      out unsigned long c);
	  float Float(in float a, 
		      inout float b, 
		      out float c);
	  double Double(in double a, 
		      inout double b, 
		      out double c);
	  boolean Boolean(in boolean a,	
			  inout boolean b,
			  out boolean c);
	  char Char(in char a, 
		    inout char b, 
		    out char c);
	  octet Octet(in octet a,
		      inout octet b,
		      out octet c);
	};  
};




