struct a {
 short f;
};

struct b {
 a f;
};

struct bar {
 struct foo {
	short f;
 } f;
};

interface test {
	bar op(in bar arg1, inout bar arg2, out bar arg3);
	b op2(in b arg1, inout b arg2, out b arg3);
};
